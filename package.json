{
  "name": "template-discord-app-bun",
  "module": "src/main.ts",
  "type": "module",
  "version": "0.1.0",
  "description": "Template for Discord Apps using Nest.js and bun",
  "scripts": {
    "dev": "bun run ./src/main.ts --watch --inspect",
    "build": "bun build ./src/main.ts --target=bun --minify --outfile=dist/main.js -e '@nestjs/microservices' -e '@nestjs/websockets*'",
    "start": "bun run ./dist/main.js",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "format": "prettier -w -c ."
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "discord.js": "^14.15.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
